  },
  "homepage": "https://github.com/HoverBaum/browserbasedgames#readme",
  "devDependencies": {
    "docdash": "^0.3.0",
    "jsdoc": "^3.4.0",
    "jsdoc-to-markdown": "^1.3.6",
    "live-server": "^1.0.0"
  }
}
