{
  "name": "browserbasedgames",
  "version": "1.0.0",
  "description": "![Alt text](/img/logo-small.png?raw=true \"Browser Based Games logo\")",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "md-docs": "node_modules/.bin/jsdoc2md src/games/standard.js --template docs/development-template.md > docs/development.md",
    "jsdoc": "./node_modules/.bin/jsdoc --configure .jsdoc.json --verbose",
    "serve-docs": "./node_modules/.bin/live-server ./docs/jsdoc",
    "docs": "npm run md-docs && npm run jsdoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HoverBaum/browserbasedgames.git"
  },
