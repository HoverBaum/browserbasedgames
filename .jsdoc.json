{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["src/games"],
        "includePattern": "(standard.js$|games.js$)",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true
    },
    "opts": {
        "destination": "./docs/jsdoc",
        "encoding": "utf8",
        "recurse": true,
        "readme": "./docs/development.md",
        "template": "./node_modules/docdash"
    }
}
